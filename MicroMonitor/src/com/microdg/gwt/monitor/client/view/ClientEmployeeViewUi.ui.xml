<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<div>This is a client employee view for <g:InlineLabel ui:field="clientName"></g:InlineLabel></div>
		<div>Name: <g:InlineLabel ui:field="employeeName"></g:InlineLabel></div>
		<div>EmailAddress: <g:InlineLabel ui:field="employeeEmailAddr"></g:InlineLabel></div>
		<g:Button styleName="btn btn-primary" ui:field="button">Edit</g:Button>
		<g:Button styleName="btn" ui:field="close">Close</g:Button>
	</g:HTMLPanel>
</ui:UiBinder> 